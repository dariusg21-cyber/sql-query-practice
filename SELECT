SELECT * FROM ARTIST
WHERE FirstName = 'PAUL';
OUTPUT
ArtistID	LastName	FirstName	Nationality	DateOfBirth	DateDeceased
3	Klee                     	Paul                     	German                        	1879	1940
18	Horiuchi                 	Paul                     	United States                 	1906	1999

SELECT * 
FROM ARTIST
ORDER BY LastName;
